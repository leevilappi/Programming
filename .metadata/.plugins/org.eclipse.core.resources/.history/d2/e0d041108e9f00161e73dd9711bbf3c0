package kierros2
import scala.collection.mutable.Buffer


class AudioEffects {
  
  val second: Int = 44100
  
  def removeVocals(sound: StereoSound) = {
    val soundData = Buffer[Int]()
    
    for(soundDataL <- sound.left){
      for(soundDataR <- sound.right){
        (soundDataL - soundDataR) / 2 
      }
    }
    
    new StereoSound("vocalsRemoved", dataLeft, dataRight)
  }
  
}