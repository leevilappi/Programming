package kierros2
import scala.collection.mutable.ArrayBuffer


object AudioEffects extends App {
  
  val second: Int = 44100
  
  def removeVocals(sound: StereoSound) = {
    val soundData = ArrayBuffer[Int]()
    
    //k채y l채pi kaikki vasemman ja oikean kanavan alkiot
       // v채henn채 vasen - oikea kanava ja jaa kahdella
      // tallenna arvo uuteen ArrayBufferiin
    
    for(i <- sound.left)
    
    
    new StereoSound("vocalsRemoved", soundData, soundData)
  }

}
